    <script type="text/javascript">
        //=================================
        function fScrollHeader() {
            var ob = document.getElementById("pageHeader");
            if (ob) {
                var topPos = fGetHeaderTopPos();
                var curScroll = (document.documentElement.scrollTop) ? document.documentElement.scrollTop : document.body.scrollTop;

                var curTop = topPos - curScroll;
                if (curTop < 0) curTop = 0;

                ob.style.position = "fixed";
                ob.style.top = curTop + "px";
            }
            fSetHeaderSpace();
        }
        //=================================
        function fGetHeaderTopPos() {
            var ar = ["CoopHeaderIframe", "wrapper"];
            var i, ob;
            var topPos = 0;
            for (i = 0; i < ar.length; i++) {
                ob = document.getElementById(ar[i]);
                if (ob) topPos += ob.offsetHeight;
            }
            return topPos;
        }
        //=================================
        function fSetHeaderSpace() {
            var ob2 = document.getElementById("pageHeaderHeight");
            if (ob2) {
                var ob = document.getElementById("pageHeader");
                var hdrH = (ob) ? ob.offsetHeight : 0;
                if (ob2) ob2.style.height = (hdrH + 10) + "px";
            }
        }
        //=================================
        fSetHeaderSpace();
        window.onscroll = fScrollHeader;
    </script>

